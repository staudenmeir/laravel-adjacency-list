{
    "name": "staudenmeir/laravel-adjacency-list",
    "description": "Recursive Laravel Eloquent relationships with CTEs",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^12.0",
        "staudenmeir/eloquent-has-many-deep-contracts": "^1.3",
        "staudenmeir/laravel-cte": "^1.12.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "larastan/larastan": "^3.0",
        "laravel/framework": "^12.0",
        "mockery/mockery": "^1.5.1",
        "orchestra/testbench-core": "^10.0",
        "phpunit/phpunit": "^11.0",
        "singlestoredb/singlestoredb-laravel": "^2.0",
        "staudenmeir/eloquent-has-many-deep": "^1.21"
    },
    "suggest": {
        "barryvdh/laravel-ide-helper": "Provide type hints for attributes and relations."
    },
    "autoload": {
        "psr-4": {
            "Staudenmeir\\LaravelAdjacencyList\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staudenmeir\\LaravelAdjacencyList\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Staudenmeir\\LaravelAdjacencyList\\IdeHelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
